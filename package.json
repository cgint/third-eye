		"@types/sharp": "0.*",
		"sharp": "0.*",
		"svelte": "5.*",
		"svelte-check": "4.*",
		"svelte-preprocess": "6.*",
		"terser": "^5.44.1",
		"typescript": "5.*",
		"vite": "^7.3.0",
		"vitest": "^4.0.16",
		"wrangler": "^4.54.0"
	},
	"type": "module",
	"dependencies": {
		"@google/generative-ai": "0.*",
		"feed": "4.*",
		"marked": "15.*"
