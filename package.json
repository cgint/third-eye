		"pprev": "npm run p && npm run preview"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "5.*",
		"@sveltejs/kit": "2.*",
		"@sveltejs/vite-plugin-svelte": "5.*",
		"@types/node": "22.*",
		"svelte": "5.*",
		"svelte-check": "4.*",
		"svelte-preprocess": "6.*",
		"typescript": "5.*",
		"vite": "6.*",
		"vitest": "2.*",
		"wrangler": "3.*",

		"@types/sharp": "*",
		"sharp": "*"
	},
	"type": "module",
	"dependencies": {
		"@google/generative-ai": "0.*",
		"feed": "4.*",
		"marked": "15.*"
	}
}
